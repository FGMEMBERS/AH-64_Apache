<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <chase-distance-m type="double" archive="y">-15.0</chase-distance-m>

  <view n="0">
  <name> Pilot </name>
    <internal archive="y">true</internal>
    <config>
      <x-offset-m archive="y">  0.100 </x-offset-m> <!--right-->
      <y-offset-m archive="y">  0.8 </y-offset-m> <!--up-->
      <z-offset-m archive="y"> -3.0 </z-offset-m> <!--back-->
      <pitch-offset-deg>-5</pitch-offset-deg>
      <default-field-of-view-deg>83</default-field-of-view-deg>
    </config>
  </view>
  
  
  
  <view n="100">
    <name>Gunner</name>
    <type>lookfrom</type>
    <internal archive="y">true</internal>
    <config>
      <from-model type="bool">true</from-model>
      <from-model-idx type="int">0</from-model-idx>
      <ground-level-nearplane-m type="double">0.01f</ground-level-nearplane-m>
      <pitch-offset-deg>-5</pitch-offset-deg>
      <x-offset-m archive="y">  0.100 </x-offset-m> <!--right-->
      <y-offset-m archive="y">  0.45 </y-offset-m> <!--up-->
      <z-offset-m archive="y"> -4.5 </z-offset-m> <!--back-->
      <pitch-offset-deg> -10.000 </pitch-offset-deg>
      <default-field-of-view-deg>84</default-field-of-view-deg>
	  <limits>
	   <left>
	  <heading-max-deg>-100</heading-max-deg>
<x-offset-threshold-deg>0</x-offset-threshold-deg>
<x-offset-max-m>0</x-offset-max-m>
</left>
<right>
<heading-max-deg>100</heading-max-deg>
<x-offset-threshold-deg>0</x-offset-threshold-deg>
<x-offset-max-m>0</x-offset-max-m>
</right>
<enabled>1</enabled>
</limits>
    </config>
  </view>

  <view n="102">
    <name>Tail Camera View</name>
    <type>lookfrom</type>
    <internal archive="y">true</internal>
    <config>
      <from-model type="bool">true</from-model>
      <from-model-idx type="int">0</from-model-idx>
      <ground-level-nearplane-m type="double">0.01f</ground-level-nearplane-m>
      <default-field-of-view-deg type="double">30</default-field-of-view-deg>
      <pitch-offset-deg>-5</pitch-offset-deg>
      <x-offset-m archive="y">  0.000 </x-offset-m> <!--Right-->
      <y-offset-m archive="y">  1.801 </y-offset-m> <!--Up-->
      <z-offset-m archive="y"> 10.637 </z-offset-m> <!--Back-->
    </config>
  </view>

  <view n="101">
    <name>TILAD</name>
    <type>lookfrom</type>
    <internal archive="y">true</internal>
    <config>
      <from-model type="bool">true</from-model>
      <from-model-idx type="int">0</from-model-idx>
      <ground-level-nearplane-m type="double">0.01f</ground-level-nearplane-m>
      <pitch-offset-deg>-5</pitch-offset-deg>
      <x-offset-m archive="y">  0 </x-offset-m> <!--right-->
      <y-offset-m archive="y">  -0.3 </y-offset-m> <!--up-->
      <z-offset-m archive="y"> -6.85 </z-offset-m> <!--back-->
      <pitch-offset-deg> -15.000 </pitch-offset-deg>
      <default-field-of-view-deg>40</default-field-of-view-deg>
	  <limits>
	   <left>
	  <heading-max-deg>-110</heading-max-deg>
<x-offset-threshold-deg>0</x-offset-threshold-deg>
<x-offset-max-m>0</x-offset-max-m>
</left>
<right>
<heading-max-deg>110</heading-max-deg>
<x-offset-threshold-deg>0</x-offset-threshold-deg>
<x-offset-max-m>0</x-offset-max-m>
</right>
<enabled>1</enabled>
</limits>
    </config>
  </view>
  
  
  <view n="103">
    <name>Pilot cam</name>
        <internal archive="y">false</internal>
        <config>
		 <from-model type="bool">true</from-model>
      <from-model-idx type="int">0</from-model-idx>
        <x-offset-m archive="y">  0.100 </x-offset-m> <!--right-->
      <y-offset-m archive="y">  1 </y-offset-m> <!--up-->
      <z-offset-m archive="y"> -5.1 </z-offset-m> <!--back-->
          <pitch-offset-deg>-2</pitch-offset-deg>
		  <heading-offset-deg>180</heading-offset-deg>
          <default-field-of-view-deg>95</default-field-of-view-deg>
        </config>
     
  </view>

  

</PropertyList>
